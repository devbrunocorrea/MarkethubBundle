{
    "name": "gpupo/markethub-bundle",
    "description": "Integração de SDKs para Marketplaces Brasileiros",
    "keywords": ["Marketplace", "B2W", "Via Varejo", "Cnova", "Netshoes", "Mercadolivre", "Symfony", "SDK"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gpupo\\MarkethubBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\MarkethubBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "symfony/http-kernel": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/config": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "gpupo/mercadolivre-sdk": "dev-master",
        "gpupo/mercadopago-sdk": "dev-master",
        "symfony/finder": "^4.1",
        "theseer/tokenizer": "^1.1",
        "symfony/cache": "^4.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1",
        "phpspec/prophecy": "~1.0"
    },
    "scripts": {
    },
    "suggest": {
        "gpupo/submarino-sdk": "Allows to integrate with B2W group",
        "gpupo/cnova-sdk": "Allows to integrate with Via Varejo group",
        "gpupo/netshoes-sdk": "Allows to integrate with Netshoes/Zattini group"
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
